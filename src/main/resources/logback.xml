<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%level] %logger{45} | %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="edu.ucla.library" level="DEBUG"/>
  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>